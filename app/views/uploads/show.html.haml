- model_class = Upload
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:user_id) + ':'
  %br
  = @upload.user_id
%p
  %strong= model_class.human_attribute_name(:upload_url) + ':'
  %br
  = @upload.upload_url


= link_to t('.back', :default => t("helpers.links.back")), uploads_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_upload_path(@upload), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), upload_path(@upload), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
